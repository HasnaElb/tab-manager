{
	"manifest_version": 3,
	"name": "Tab Manager",
	"version": "1.0",
	"description": "Manage and organize your open tabs.",
	"permissions": ["tabs", "storage"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"48": "icon.png"
	}
}
